    <servlet>
    	<!--给你的servlrt取名，任意的-->
      <servlet-name>hello</servlet-name>
      <!--指明servlet的路径，（包名+类名）-->
      <servlet-class>com.tsinghua.Hello</servlet-class>
    </servlet>
    
    <servlet-mapping>
    		<!--给你的servlrt取名，任意的-->
        <servlet-name>hello</servlet-name>
        <!--这是在浏览器中输入的访问该servlet的url-->
        <url-pattern>/sp</url-pattern>
    </servlet-mapping>
    
